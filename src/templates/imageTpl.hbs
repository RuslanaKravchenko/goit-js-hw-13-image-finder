{{#each this}}
<li class="gallery__item ">
  <div class="photo-card">
    <img src="{{webformatURL}}" data-img="{{largeImageURL}}" alt="{{tags}}" />

    <div class="stats">
      <p class="stats-item likes">
        <i class="material-icons">thumb_up</i>
        {{likes}}
      </p>
      <p class="stats-item views">
        <i class="material-icons">visibility</i>
        {{views}}
      </p>
      <p class="stats-item">
        <i class="material-icons comments">comment</i>
        {{comments}}
      </p>
      <p class="stats-item">
        <i class="material-icons downloads">cloud_download</i>

        {{downloads}}
      </p>
    </div>
  </div>
</li>
{{/each}}